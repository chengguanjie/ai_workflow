{
  "name": "ai-workflow",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "check:node": "node scripts/check-node-version.js",
    "check:routes": "node scripts/check-routes.js",
    "build": "pnpm check:routes && prisma generate && next build",
    "start": "next start",
    "lint": "pnpm lint:app",
    "lint:app": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:scripts": "eslint \"scripts/**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "pretest": "pnpm check:node",
    "test": "vitest --config vitest.config.mts --run",
    "test:watch": "vitest --config vitest.config.mts",
    "test:coverage": "vitest --config vitest.config.mts --run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "init:admin": "node --import tsx scripts/init-platform-admin.ts",
    "seed:templates": "node --import tsx scripts/seed-templates.ts",
    "workflow:trace": "node --import tsx scripts/trace-and-fix-workflow.ts",
    "workflow:trace:api": "node --import tsx scripts/trace-and-fix-workflow-v1-api.ts",
    "workflow:list:api": "node --import tsx scripts/list-workflows-v1-api.ts",
    "security:audit": "bash scripts/security-audit.sh",
    "pue:30d": "node --import tsx scripts/pue-30d-report.ts"
  },
